if [[ $# == 1 ]]; then
    echo '+ v $(qol' "$@"")" +'"'/$1'"' >&2
    # shellcheck disable=SC2046  # Quote this to prevent word splitting
    v $(qol "$@") +"/$1"
else
    echo '+ v $(qol' "$@"")" >&2
    # shellcheck disable=SC2046  # Quote this to prevent word splitting
    v $(qol "$@")
fi

# note: Bash unsets the 'qol' "${ALTPWDS[@]}" unless you run this Code as a Bash Func
